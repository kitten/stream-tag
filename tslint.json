{
  "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["src/tests/**/*", "example/**/*"]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "member-ordering": false,
    "ordered-imports": false,
    "member-access": false,
    "no-submodule-imports": false
  }
}
